{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect-brk",
        "-A",
        "--allow-plugin",
        "--unstable",
        "main.ts"
      ],
      "outputCapture": "std",
      "port": 9229
    }
  ]
}
